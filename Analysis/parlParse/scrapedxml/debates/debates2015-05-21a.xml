<speech id="uk.org.publicwhip/debate/2015-05-21a.26.0" nospeaker="true" colnum="26"  time="011:15:00" url="http://www.publications.parliament.uk/pa/cm201516/cmhansrd/cm150521/debtext/150521-0001.htm#150521-0001.htm_spnew1">
	<p pid="a.26.0/1" class="italic">Sitting suspended.</p>
	<p pid="a.26.0/2" class="italic">On resuming&#8212;</p>
</speech>

<speech id="uk.org.publicwhip/debate/2015-05-21a.26.1" person_id="uk.org.publicwhip/person/10040" speakername="Mr Speaker" colnum="26"  time="011:15:00" url="http://www.publications.parliament.uk/pa/cm201516/cmhansrd/cm150521/debtext/150521-0001.htm#150521-0001.htm_spnew1">
